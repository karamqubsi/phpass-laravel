{
    "name": "karamqubsi/phpass-laravel",
    "description": "PHPass Hashing integration with Laravel 8",
    "keywords": ["phpass", "laravel", "hashing", "password"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kenneth Sungcaya",
            "email": "ksungcaya@gmail.com"
        },
        {
            "name": "Karam Qubsi",
            "email": "karamqubsi@gmail.com"
        }
    ],
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "require": {
        "php": ">=7.3"
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/hashing" :  "^8.0",
        "hautelook/phpass": "*"
    },
    "autoload": {
        "psr-4": {
            "Karamqubsi\\Phpass\\": "src/Karamqubsi/Phpass"
        }
    },
    "minimum-stability" : "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Karamqubsi\\Phpass\\PhpassHashServiceProvider"
            ]
        }
    }
}
